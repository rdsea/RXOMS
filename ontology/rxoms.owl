#################################################################
#    Object Properties
#################################################################

###  http://www.aalto.fi/rxoms#affectDevice
:affectDevice rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Incident ;
              rdfs:range :NetworkDevice .


###  http://www.aalto.fi/rxoms#causedBy
:causedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Incident ;
          rdfs:range :NetworkDevice .


###  http://www.aalto.fi/rxoms#connectedToDevice
:connectedToDevice rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/connectedTo> ;
                   rdfs:domain :Port ;
                   rdfs:range :NetworkDevice .


###  http://www.aalto.fi/rxoms#hasConfiguration
:hasConfiguration rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain :SecurityTool ;
                  rdfs:range :Configuration .


###  http://www.aalto.fi/rxoms#hasDictionaryItem
:hasDictionaryItem rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :Dictionary ;
                   rdfs:range :DictionaryItem .


###  http://www.aalto.fi/rxoms#hasIPAddress
:hasIPAddress rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :NetworkDevice ;
              rdfs:range :IPAddress .


###  http://www.aalto.fi/rxoms#hasMLModel
:hasMLModel rdf:type owl:ObjectProperty ;
            rdfs:domain :SecurityTool ;
            rdfs:range :MachineLearningModel .


###  http://www.aalto.fi/rxoms#hasMitigation
:hasMitigation rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :mitigates .


###  http://www.aalto.fi/rxoms#hasPerformance
:hasPerformance rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :MachineLearningModel ;
                rdfs:range :Performance .


###  http://www.aalto.fi/rxoms#hasPort
:hasPort rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasPart> ;
         owl:inverseOf :isPortOf ;
         rdfs:domain :NetworkDevice ;
         rdfs:range :Port .


###  http://www.aalto.fi/rxoms#hasProtectionRule
:hasProtectionRule rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :ProtectionService ;
                   rdfs:range :ProtectionRule .


###  http://www.aalto.fi/rxoms#hasTrainingParam
:hasTrainingParam rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain :MachineLearningModel ;
                  rdfs:range :TrainingParam .


###  http://www.aalto.fi/rxoms#isPortOf
:isPortOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/partOf> .


###  http://www.aalto.fi/rxoms#mitigates
:mitigates rdf:type owl:ObjectProperty ;
           rdfs:domain :Mitigation ;
           rdfs:range :Incident .


###  http://www.aalto.fi/rxoms#monitorDevice
:monitorDevice rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :DectectionService ;
               rdfs:range :NetworkDevice .


###  http://www.aalto.fi/rxoms#protectsDevice
:protectsDevice rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :ProtectionService ;
                rdfs:range :NetworkDevice .


###  http://www.aalto.fi/rxoms#provideAPI
:provideAPI rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :ProtectionService ;
            rdfs:range :API .


###  http://www.aalto.fi/rxoms#serviceOf
:serviceOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :DectectionService ,
                       :ProtectionService ;
           rdfs:range :SecurityTool .


###  http://www.aalto.fi/rxoms#subTechniques
:subTechniques rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Incident ;
               rdfs:range :Incident .


###  http://www.aalto.fi/rxoms#useAPI
:useAPI rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain :Mitigation ;
        rdfs:range :API .


