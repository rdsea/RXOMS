#################################################################
#    Classes
#################################################################

###  http://www.aalto.fi/rxoms#API
:API rdf:type owl:Class ;
     rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> .


###  http://www.aalto.fi/rxoms#Configuration
:Configuration rdf:type owl:Class ;
               rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> .


###  http://www.aalto.fi/rxoms#DectectionService
:DectectionService rdf:type owl:Class ;
                   rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/SoftwareArtefact> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :monitorDevice ;
                                     owl:someValuesFrom :NetworkDevice
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :serviceOf ;
                                     owl:someValuesFrom :SecurityTool
                                   ] ;
                   owl:disjointWith :ProtectionService .


###  http://www.aalto.fi/rxoms#Dictionary
:Dictionary rdf:type owl:Class ;
            rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasDictionaryItem ;
                              owl:someValuesFrom :DictionaryItem
                            ] .


###  http://www.aalto.fi/rxoms#DictionaryConfiguration
:DictionaryConfiguration rdf:type owl:Class ;
                         rdfs:subClassOf :Configuration ,
                                         :Dictionary .


###  http://www.aalto.fi/rxoms#DictionaryItem
:DictionaryItem rdf:type owl:Class ;
                rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> .


###  http://www.aalto.fi/rxoms#Gateway
:Gateway rdf:type owl:Class ;
         rdfs:subClassOf :NetworkDevice .


###  http://www.aalto.fi/rxoms#Host
:Host rdf:type owl:Class ;
      rdfs:subClassOf :NetworkConfiguration .


###  http://www.aalto.fi/rxoms#IPAddress
:IPAddress rdf:type owl:Class ;
           rdfs:subClassOf :NetworkConfiguration .


###  http://www.aalto.fi/rxoms#Incident
:Incident rdf:type owl:Class ;
          rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Event> .


###  http://www.aalto.fi/rxoms#MachineLearningModel
:MachineLearningModel rdf:type owl:Class ;
                      rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/SoftwareArtefact> ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasConfiguration ;
                                        owl:someValuesFrom :Configuration
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasPerformance ;
                                        owl:someValuesFrom :Performance
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasTrainingParam ;
                                        owl:someValuesFrom :TrainingParam
                                      ] .


###  http://www.aalto.fi/rxoms#Mitigation
:Mitigation rdf:type owl:Class ;
            rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/InformationObject> .


###  http://www.aalto.fi/rxoms#NetworkConfiguration
:NetworkConfiguration rdf:type owl:Class ;
                      rdfs:subClassOf :Configuration .


###  http://www.aalto.fi/rxoms#NetworkDevice
:NetworkDevice rdf:type owl:Class ;
               rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasIPv4Address ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasIPv6Address ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] .


###  http://www.aalto.fi/rxoms#Performance
:Performance rdf:type owl:Class ;
             rdfs:subClassOf :Dictionary .


###  http://www.aalto.fi/rxoms#Port
:Port rdf:type owl:Class ;
      rdfs:subClassOf :NetworkConfiguration ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :connectedToDevice ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :NetworkDevice
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :hasPortNumber ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:integer
                      ] .


###  http://www.aalto.fi/rxoms#Process
:Process rdf:type owl:Class ;
         rdfs:subClassOf :NetworkConfiguration .


###  http://www.aalto.fi/rxoms#Program
:Program rdf:type owl:Class ;
         rdfs:subClassOf :NetworkConfiguration .


###  http://www.aalto.fi/rxoms#ProtectionRule
:ProtectionRule rdf:type owl:Class ;
                rdfs:subClassOf :DictionaryConfiguration .


###  http://www.aalto.fi/rxoms#ProtectionService
:ProtectionService rdf:type owl:Class ;
                   rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/SoftwareArtefact> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasConfiguration ;
                                     owl:someValuesFrom :Configuration
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasProtectionRule ;
                                     owl:someValuesFrom :ProtectionRule
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :protectsDevice ;
                                     owl:someValuesFrom :NetworkDevice
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :serviceOf ;
                                     owl:someValuesFrom :SecurityTool
                                   ] .


###  http://www.aalto.fi/rxoms#SecurityTool
:SecurityTool rdf:type owl:Class ;
              rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/SoftwareArtefact> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasConfiguration ;
                                owl:someValuesFrom :Configuration
                              ] .


###  http://www.aalto.fi/rxoms#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf :NetworkConfiguration .


###  http://www.aalto.fi/rxoms#Switch
:Switch rdf:type owl:Class ;
        rdfs:subClassOf :NetworkDevice .


###  http://www.aalto.fi/rxoms#TrainingParam
:TrainingParam rdf:type owl:Class ;
               rdfs:subClassOf :DictionaryConfiguration .


###  http://www.aalto.fi/rxoms#User
:User rdf:type owl:Class ;
      rdfs:subClassOf :NetworkConfiguration .


